package arbitrary

import ev "gitlab.inf.ethz.ch/arquintl/prototrace/event"
import "gitlab.inf.ethz.ch/arquintl/prototrace/label"
import p "gitlab.inf.ethz.ch/arquintl/prototrace/principal"
import tm "gitlab.inf.ethz.ch/arquintl/prototrace/term"
import tr "gitlab.inf.ethz.ch/arquintl/prototrace/trace"
import u "gitlab.inf.ethz.ch/arquintl/prototrace/usage"

// utility package to return an arbitrary value which can then be used for forall introductions.
// these Go functions correspond to Viper methods that do not have any postcondition and thus do not constrain
// their return value.

decreases
func GetArbTerm() tm.Term

decreases
func GetArbTraceEntry() tr.TraceEntry

decreases
func GetArbLabel() label.SecrecyLabel

decreases
func GetArbPrincipal() p.Principal

decreases
func GetArbId() p.Id

decreases
func GetArbEvent() ev.Event

decreases
func GetArbUsage() u.Usage

decreases
func GetArbString() string

decreases
func GetArbUInt32() uint32

